	TPCheck: function() {
		var townid,
			_currentwp = me.areaid;

		if(NTTMGR_CheckScrolls(1) > 4) {
			return;
		}

		switch(me.act) {
			case 1:
				townid = 1;
			break;
			case 2:
				townid = 40;
			break;
			case 3:
				townid = 75;
			break;
			case 4:
				townid = 103;
			break;
			case 5:
				townid = 109;
			break;
		}

		NTM_TakeWaypoint(townid);
		NTTMGR_TownManager();

		NTTM_TownMove("waypoint");
		return;
	},

	Waypoints: function() {
		var i, _start, _wppos,
			_wps = [3, 4, 5, 6, 27, 29, 32, 35, 48, 42, 57, 43, 44, 52, 74, 46, 76, 77, 78, 79, 80, 81, 83, 101, 106, 107, 111, 112, 113, 115, 123, 117, 118, 129];

		if(!NTTM_CheckAct(1)) {
			Common.LogError("NTTM_CheckAct()");
			return false;
		}

		switch(me.act) {
			case 1:
				_start = 0;
				break;
			case 2:
				_start = 8;
				break;
			case 3:
				_start = 16;
				break;
			case 4:
				_start = 24;
				break;
			case 5:
				_start = 26;
				break;
		}

		NTTMGR_TownManager();
		NTTM_TownMove("waypoint");

		for(i = _start; i < _wps.length; i += 1) {
			if(NTM_TakeWaypoint(_wps[i])) {
				NTM_MoveTo(me.areaid, me.x - 6, me.y - 6);
				if(NTM_MakeTP()) {					
					_wppos = [me.x, me.y];

					Say(Messages.waypoint + GetArea().name);

					if(!me.GetState(51)) {
						NTP_DoPrecast(true);
					}

					while(!NTC_IsQuesterIn()) {
						NTC_TPGuard(30);
						NTM_MoveTo(me.areaid, _wppos[0], _wppos[1]);
					}

					while(!NTC_IsQuesterOut(me.areaid)) {
						NTC_Delay(500);
					}

					TPCheck();
				}
			}
		}
		return true;
	}
