function NTMain() {
	Include("libs/common/NTCommon.ntl");
	Include("NTRush/funcs/RushCommon.ntl");
	Common.IncludeFuncs();
	NTC_IncludeLibs();
	Config.Load();

	NT_LoadConfig();
	NTSI_LoadNIPFiles("NTBot/item_configs");

	NTA_Initialize();

	if (!Extras.Tome()) {
		Common.LogError("Tome()");
		Script.SendMsg("OverWatch.ntj", "Tome()");
		return;
	}

	Script.SendMsg("OverWatch.ntj", "SCRIPT_END"); 
}